{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "ESNext",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"]
    },
    "types": ["cypress"]
  },
  "include": [
    "cypress/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
